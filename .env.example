# Gemini API key for image generation access
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: Authentication token for MCP
MCP_AUTH_TOKEN=your_auth_token_here

# Web server port (default: 3020)
PORT=3020

# Output directory for saved images (default: ./generated-images)
# OUTPUT_DIR=./your-custom-output-path

# Log level (debug, info, warn, error)
LOG_LEVEL=info